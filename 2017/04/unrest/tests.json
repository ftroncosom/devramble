{
  "postList": {
	"request": {
		"endpoint": "/posts",
		"headers": {
			"Accept": "application/xml"
		}
	}
  },
  "singlePost": {
	"request": {
		"endpoint": "/posts/${postId}"
	}
  },
  "createPost": {
	"request": {
		"endpoint": "/posts",
		"headers": {
			"Content-Type": "application.json"
		},
		"body": {
			"content": "{\"userId\": 1, \"title\": \"some title\", \"body\": \"some body\"}"
		}
	}
  },
  "createPostFromFile": {
		"request": {
			"endpoint": "/posts",
			"headers": {
				"Content-Type": "application.json"
			},
			"body": {
				"file": "post_data.json"
			}
	}
  }
}